{
  "name": "dpass",
  "displayName": "dpass",
  "version": "0.1.0",
  "description": "Diggsey's Password Manager",
  "main": "index.js",
  "scripts": {
    "build": "tsc && vite build",
    "watch": "vite build --watch --minify false --mode development",
    "dev": "vite",
    "serve:firefox": "web-ext run --start-url \"about:debugging#/runtime/this-firefox\"  --source-dir ./dist/",
    "serve:chrome": "web-ext run -t chromium --start-url \"https://google.com\" --source-dir ./dist/"
  },
  "author": "Diggory Blake",
  "license": "MIT",
  "devDependencies": {
    "@samrum/vite-plugin-web-extension": "^3.0.0",
    "@types/chrome": "^0.0.206",
    "@types/webextension-polyfill": "^0.9.2",
    "copy-webpack-plugin": "^11.0.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "webextension-polyfill": "^0.10.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@msgpack/msgpack": "^2.8.0",
    "htm": "^3.1.1",
    "preact": "^10.11.3"
  }
}